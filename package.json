{
  "name": "ts-force",
  "version": "1.1.1",
  "description": "a typescript client for connecting with salesforce APIs",
  "main": "build/index.js",
  "typings": "build/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChuckJonas/ts-force.git"
  },
  "keywords": [
    "salesforce",
    "typescript",
    "rest"
  ],
  "contributors": [
    "Charlie Jonas",
    "David Staley"
  ],
  "license": "BSD-3-Clause",
  "scripts": {
    "clean": "rm -r -f ./build",
    "clean-build": "npm run clean && npm run build",
    "link": "npm run clean-build && npm link",
    "pretest": "npm run clean && tsc",
    "test": "mocha -r dotenv/config dotenv_config_path=.env.test",
    "build": "tsc -p tsconfig.build.json",
    "watch": "mocha-typescript-watch",
    "prepublishOnly": "npm run clean-build"
  },
  "engines": {
    "node": ">=4.5"
  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/mocha": "^2.2.43",
    "@types/nock": "^8.2.1",
    "@types/node": "^8.0.4",
    "@types/qs": "^6.5.0",
    "chai": "^4.1.2",
    "mocha": "^3.5.2",
    "mocha-typescript": "^1.1.8",
    "nock": "^9.0.14",
    "source-map-support": "^0.4.18",
    "tslint": "^5.4.3",
    "tslint-config-standard": "^6.0.1",
    "typescript": "^2.8.1",
    "dotenv": "^4.0.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "lodash.clonedeep": "^4.5.0",
    "qs": "^6.5.1",
    "reflect-metadata": "^0.1.10",
    "tslib": "^1.6.0"
  }
}
